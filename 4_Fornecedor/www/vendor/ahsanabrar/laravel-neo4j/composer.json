{
    "name": "ahsanabrar/laravel-neo4j",
    "description": "Laravel wrapper for Neo4j graph database.",
    "keywords": [
        "laravel",
        "neo4j",
        "graph",
        "database",
        "laravel-neo4j"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ahsan Abrar",
            "email": "iahsanabrar@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "graphaware/neo4j-php-client": "4.8.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "1.0"
    },
    "autoload": {
        "psr-4": {
            "Ahsan\\Neo4j\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ahsan\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Ahsan\\Neo4j\\Neo4jServiceProvider"
            ]
        }
    }
}
